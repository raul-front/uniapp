{
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "uni-app"
      }
    }
  ],
  "subPackages": [
    // exampleStart
    {
      "root": "pagesK",
      "pages": [
        {
          "path": "index",
          "style": {
            "navigationBarTitleText": "组件"
          }
        },
        {
          "path": "base/form",
          "style": {
            "navigationBarTitleText": "表单"
          }
        },
        {
          "path": "page/refreshPageList",
          "style": {
            "navigationBarTitleText": "页面下拉刷新",
            "enablePullDownRefresh": true
          }
        },
        {
          "path": "page/refreshScrollViewList",
          "style": {
            "navigationBarTitleText": "ScrollView下拉刷新"
          }
        },
        {
          "path": "page/pageFooterHandle",
          "style": {
            "navigationBarTitleText": "页面底部有操作按钮"
          }
        },
        {
          "path": "page/index",
          "style": {
            "navigationBarTitleText": "索引列表"
          }
        }
      ]
    },
    // exampleEnd
    {
      "root": "pagesA",
      "pages": []
    },
    {
      "root": "pagesB",
      "pages": []
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8",
    "backgroundColorTop": "#F8F8F8",
    "backgroundColorBottom": "#F8F8F8"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "^kg-(.*)": "@/components/kg/$1/index.vue",
      "^home-(.*)": "@/components/home/$1/index.vue"
    }
  }
}
